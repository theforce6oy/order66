import java.util.Scanner;

public class ContinueQ1_123p {

	public static void main(String[] args) {
		
	Scanner scan = new Scanner(System.in);
	System.out.println("import int below");
	int n = scan.nextInt();
	
	int a = 0;
	int total = 0;
	
	while(a<n) {
		a += 1;
		  if(a%2 /= 0) {
		  total += a;
		  System.out.println("1부터" + a + "까지 총합은 :" +total+"입니다.");
		   continue;
		   }
		   
		 n++; // 다 돌았으면 바퀴수 증
	}
	}
}